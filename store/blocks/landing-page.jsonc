            "width": "70%"
        },
        "children": [
            "flex-layout.row#lp1-products"
        ]
    },

    "flex-layout.row#lp1-products": {
        "children": [
            "shelf#lp1-products"
        ]
    },

    "shelf#lp1-products": {
        "blocks": [
            "product-summary.shelf#sku"
        ],
        "props": {
            "category": "1/9",
            "orderBy": "OrderByTopSaleDESC",
            "productList": {
                "maxItems": 10,
                "itemsPerPage": 3,
                "scroll": "BY_PAGE",
                "arrows": true,
                "showTitle": false,
                "hideUnavailableItems": true,
                "summary": {
                    "isOneClickBuy": false,
                    "showBadge": true,
                    "badgeText": "OFF",
                    "buyButtonText": "Comprar",
                    "displayBuyButton": "displayButtonHover",
                    "showCollections": false,
                    "showListPrice": true,
                    "showLabels": false,
                    "showInstallments": true,
                    "showSavings": true,
                    "name": {
                        "showBrandName": false,
                        "showSku": false,
                        "showProductReference": false
                    }
                }
            }
        }
    },

    "flex-layout.col#lp1-card": {
        "props": {
            "width": "30%",
            "verticalAlign": "middle",
            "horizontalAlign": "right"
        },
        "children": [
