            "responsive-layout.phone#lp2",
            "flex-layout.row#landing-page2",
            "responsive-layout.phone#lp3",
            "flex-layout.row#landing-page3"
        ]
    },

    "flex-layout.row#banner-lp": {
        "props": {
            "fullWidth": true,
            "horizontalAlign": "center"
        },
        "children": [
            "image#banner-lp"
        ]
    },

    "image#banner-lp": {
        "props": {
            "src": "https://bookstoretheme.vteximg.com.br/assets/vtex.file-manager-graphql/images/bannerlp___c4ffc133e9dbf6562aadefc3f7e97f11.png",
            "alt": "Landing Page Banner"
        }
    },

    "flex-layout.row#landing-page1": {
        "props": {
            "horizontalAlign": "center"
        },
        "children": [
            "flex-layout.col#lp1-products",
            "responsive-layout.desktop#lp1"
        ]
    },

    "responsive-layout.desktop#lp1": {
        "children": [
            "flex-layout.col#lp1-card"
        ]
    },

    "responsive-layout.phone#lp1": {
        "children": [
            "flex-layout.row#lp1-card-phone"
        ]
    },

    "flex-layout.col#lp1-products": {
